													.map-aside__contacts
														.map-aside__contact
															a(href="tel:79867635212").map-aside__contact-elem
																+icon('phone-ring')
																| +7 (985) 760 78 11
														.map-aside__contact
															.map-aside__contact-elem
																+icon('phone-fax')
