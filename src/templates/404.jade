								.social 
									a(href="#").social__link: img(src="images/fb.png")
									a(href="#").social__link: img(src="images/tw.png")
									a(href="#").social__link: img(src="images/yo.png")
									a(href="#").social__link: img(src="images/in.png")
									a(href="#").social__link: img(src="images/vk.png")

							.footer__app
								.footer__label Наши приложения
								.app__list
									.app__link.gplay
										a(href="#")
											img(src="images/gplay.svg" width="148px" height="37px")
									.app__link.appstore
										a(href="#")
											img(src="images/appstore.svg" width="148px" height="40px")

			.footer__bottom
				.container
					.footer__copy © 2016 VEKA



		include _parts/_scripts